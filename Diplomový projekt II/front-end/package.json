    "@types/axios": "^0.14.0",
    "axios": "^1.5.1",
    "firebase": "^10.5.0",
    "neovis.js": "^2.1.0",
    "papaparse": "^5.4.1",
    "pinia": "^2.1.6",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primevue": "^3.36.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
